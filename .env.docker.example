# Telegram Bot Token
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
# Leave blank to use polling mode
TELEGRAM_WEBHOOK_URL=

# Model Provider (ollama, google, groq, or openai)
MODEL_PROVIDER=ollama
MODEL_VISION_PROVIDER=ollama # Optional, override for vision, if not, fallback to MODEL_PROVIDER
MODEL_UTILITY_PROVIDER=ollama # Optional, override for tool, if not, fallback to MODEL_PROVIDER

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL_NAME=gpt-4o
OPENAI_UTILITY_MODEL_NAME=gpt-4o-mini
OPENAI_VISION_MODEL_NAME=gpt-4o

# Ollama Configuration
OLLAMA_API_URL=http://ollama:11434
OLLAMA_MODEL_NAME=llama3.2
OLLAMA_UTILITY_MODEL_NAME=qwen2.5:1b
OLLAMA_VISION_MODEL_NAME=llama-3.2-vision

# Google Configuration
GOOGLE_API_KEY=your_google_api_key
GOOGLE_MODEL_NAME=gemini-1.5-flash
GOOGLE_UTILITY_MODEL_NAME=gemini-1.5-flash-8b
GOOGLE_VISION_MODEL_NAME=gemini-1.5-flash

# Groq Configuration
GROQ_API_KEY=your_groq_api_key
GROQ_MODEL_NAME=llama-3.3-70b-versatile
GROQ_UTILITY_MODEL_NAME=llama-3.1-8b-instant
GROQ_VISION_MODEL_NAME=llama-3.2-90b-vision-preview

# Redis Configuration
REDIS_URL=redis://redis:6379

# Search API Configuration (tavily, brave, duckduckgo) Recommended: tavily
SEARCH_PROVIDER=tavily
SEARCH_MAX_RESULTS=3
TAVILY_API_KEY=your_tavily_api_key
BRAVE_SEARCH_API_KEY=your_brave_search_api_key

# Optional LangSmith Configuration
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
# LANGCHAIN_API_KEY=your_langsmith_api_key
# LANGCHAIN_PROJECT=your_project_name